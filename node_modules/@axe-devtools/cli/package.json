{
  "name": "@axe-devtools/cli",
  "version": "4.4.3",
  "description": "Axe Devtools CLI Application",
  "scripts": {
    "test": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/*.test.ts' 'lib/**/*.test.ts' 'scripts/**/*.test.js'",
    "test:bin:bulk-reports": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/bulk-reports.test.ts'",
    "test:bin:bulk-spec": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/bulk-spec.test.ts'",
    "test:bin:cli": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/cli.test.ts'",
    "test:bin:config-selenium": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/config-selenium.test.ts'",
    "test:bin:reporter": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/reporter.test.ts'",
    "test:bin:ruleset": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/ruleset.test.ts'",
    "test:bin:spec": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/spec.test.ts'",
    "test:bin:uri": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'bin/uri.test.ts'",
    "test:lib": "cross-env AXE_DEVTOOLS_PREF_PERSISTENCE=false mocha 'lib/*.test.ts'",
    "coverage": "nyc npm run test",
    "prepare": "npm run build",
    "build": "npm run build:bin-lib && npm run build:scripts && npm run build:pkgs && npm run rename:pkg",
    "build:bin-lib": "tsc",
    "build:pkgs": "npm run pkg",
    "prebuild:scripts": "mkdirp dist/scripts",
    "build:scripts": "browserify --transform babelify scripts/global-dismiss-action.js > dist/scripts/global-dismiss-action.js",
    "prepublishOnly": "npm run build",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' 'bin/**/*.ts'",
    "start-example": "http-server example/public -p 5000 -c-1",
    "acceptance": "node e2e/version.js && node e2e/spec.js && node e2e/uri.js",
    "pkg": "pkg dist/bin/cli.js --config=package.json --out-path=pkgs",
    "upload-binaries": "bash ../../.circleci/upload-binary.sh",
    "postpublish": "npm run upload-binaries",
    "rename:pkg": "node rename.js",
    "preinstall": "node preinstall.js",
    "postuninstall": "node postuninstall.js"
  },
  "bin": {
    "axe": "cli.js"
  },
  "files": [
    "pkgs/@axe-devtools/axe-linux",
    "pkgs/@axe-devtools/axe-macos",
    "pkgs/@axe-devtools/axe-win.exe",
    "postuninstall.js",
    "preinstall.js"
  ],
  "preferGlobal": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@axe-core/cli": "^4.4.3",
    "@axe-devtools/logger": "^4.4.0",
    "@axe-devtools/reporter": "^4.4.0",
    "@axe-devtools/reporter-csv": "^4.4.0",
    "@axe-devtools/script-builder": "^4.4.3",
    "@axe-devtools/webdriverjs": "^4.4.3",
    "@deque/attest-standards": "^4.4.3",
    "@deque/metrics-library": "^3.4.0",
    "@deque/reports-link": "1.6.0",
    "@types/chai": "^4.3.1",
    "@types/chai-fs": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/mockery": "^1.4.30",
    "@types/node": "^18.0.3",
    "@types/papaparse": "^5.3.2",
    "@types/selenium-webdriver": "^4.1.1",
    "@types/sinon": "^10.0.12",
    "@types/test-listen": "^1.1.0",
    "axe-core": "^4.4.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "8.0.0",
    "browserify": "16.2.2",
    "chai": "^4.3.6",
    "chai-fs": "^2.0.0",
    "chalk": "2.4.2",
    "child_process": "^1.0.2",
    "chromedriver": "latest",
    "circular-json": "^0.3.3",
    "cli-spinner": "^0.2.10",
    "clone": "^2.1.2",
    "clui": "^0.3.6",
    "colors": "^1.4.0",
    "commander": "^9.3.0",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.4",
    "delay": "^5.0.0",
    "dot": "^1.1.3",
    "execa": "^5.1.1",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "graceful-fs": "^4.2.10",
    "http-server": "^14.1.1",
    "inquirer": "^8.2.3",
    "jfrog-cli-go": "^1.53.4",
    "js-schema": "^1.0.1",
    "jsdom": "^20.0.0",
    "json2csv": "^5.0.7",
    "jsonschema": "^1.4.1",
    "log-symbols": "^4.1.0",
    "mkdirp": "^1.0.4",
    "mocha": "^10.0.0",
    "mockery": "^2.1.0",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "nock": "^13.2.8",
    "nyc": "^15.1.0",
    "p-retry": "^4.6.1",
    "p-timeout": "^4.1.0",
    "papaparse": "^5.3.2",
    "parsimmon": "^1.18.1",
    "path": "^0.12.7",
    "pify": "^6.0.0",
    "pkg": "^5.7.0",
    "preferences": "^2.0.2",
    "require-from-string": "^2.0.2",
    "selenium-webdriver": "4.3.0",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "tempy": "^1.0.1",
    "test-listen": "^1.1.0",
    "ts-node": "^10.8.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^4.7.4",
    "typescript-cached-transpile": "0.0.6",
    "uniq-selector": "^0.2.3",
    "valid-url": "^1.0.9",
    "validate-arguments": "0.0.8"
  },
  "nyc": {
    "checkCoverage": true,
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 90,
    "branches": 78,
    "functions": 90,
    "lines": 90,
    "exclude": [
      "dist",
      "coverage",
      "**/*.test.js",
      "**/*.test.ts",
      "testutils",
      "testutils/bulk-report-utils",
      "lib/ruleset/tests/",
      "bin/spec.ts"
    ]
  },
  "pkg": {
    "assets": [
      "dist/scripts/*.js",
      "node_modules/@axe-devtools/reporter/dist/**/*",
      "node_modules/@axe-devtools/webdriverjs/node_modules/selenium-webdriver/**/*",
      "node_modules/axe-core/*.js",
      "node_modules/@axe-core/cli/node_modules/selenium-webdriver/**/*",
      "node_modules/selenium-webdriver/**/*",
      "node_modules/@deque/reports-link/dist/**"
    ],
    "targets": [
      "node12-linux-x64",
      "node12-macos-x64",
      "node12-win-x64"
    ]
  },
  "gitHead": "297c4f753a8f339e2182ac184052191d0a70a759"
}
